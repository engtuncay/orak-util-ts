{
  "name": "orak-util-ts",
  "version": "0.0.42",
  "description": "Orak Software Utility Typescript Library",
  "type": "module",
  "main": "dist/orak-util-ts.cjs",
  "module": "dist/orak-util-ts.js",
  "types": "dist/orak-util-ts.d.ts",
  "files": [
    "dist"
  ],
  "fiBuildComment": "Add new class to index.ts",
  "scripts": {
    "ku-install": "yarn",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup",
    "ku-publish": "yarn publish --no-git-checks",
    "ku-buildPub": "tsup && yarn publish --no-git-checks",
    "pnpm-link-orak-util-ts": "pnpm link --global",
    "pn-unlink-orak-util-ts": "pnpm unlink orak-util-ts",
    "pn-list-global": "pnpm list -g"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "author": "Tuncay Orak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/engtuncay/orak-util-ts.git"
  },
  "bugs": {
    "url": "https://github.com/engtuncay/orak-util-ts.git/issues"
  },
  "homepage": "https://github.com/engtuncay/orak-util-ts#readme",
  "devDependencies": {
    "solid-js": "^1.9.5",
    "terser": "^5.39.0",
    "tsup": "^8.3.0",
    "typescript": "^5.2.2",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "axios": "^1.8.4"
  }
}
