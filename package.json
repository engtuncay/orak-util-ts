{
  "name": "orak-util-ts",
  "version": "0.0.42",
  "description": "Orak Software Utility Typescript Library",
  "type": "module",
  "main": "dist/orak-util-ts.cjs",
  "module": "dist/orak-util-ts.js",
  "types": "dist/orak-util-ts.d.ts",
  "files": [
    "dist"
  ],
  "fiBuildComment": "Add new class to index.ts",
  "scripts": {
    "install-okuts": "yarn install",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup",
    "publish-okuts": "yarn publish --no-git-checks",
    "buildPub-okuts": "tsup && yarn publish --no-git-checks",
    "link-okuts": "yarn link",
    "unlink-okuts": "yarn unlink orak-util-ts",
    "list-global": "yarn list -g"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "author": "Tuncay Orak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/engtuncay/orak-util-ts.git"
  },
  "bugs": {
    "url": "https://github.com/engtuncay/orak-util-ts.git/issues"
  },
  "homepage": "https://github.com/engtuncay/orak-util-ts#readme",
  "devDependencies": {
    "solid-js": "^1.9.5",
    "terser": "^5.39.0",
    "tsup": "^8.3.0",
    "typescript": "^5.2.2",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "axios": "^1.8.4"
  }
}
